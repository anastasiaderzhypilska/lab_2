def min_eating_speed(piles, H):
    def hours_needed(piles, k):
        total_hours = 0
        for bananas in piles:
            hours = bananas // k
            if bananas % k != 0:
                hours += 1
            total_hours += hours
        return total_hours

    left = 1
    right = 0
    for pile in piles:
        if pile > right:
            right = pile

    result = right

    while left <= right:
        mid = (left + right) // 2
        needed_hours = hours_needed(piles, mid)

        if needed_hours <= H:
            result = mid
            right = mid - 1
        else:
            left = mid + 1

    return result

if __name__ == '__main__':
    piles = [3, 6, 7, 11]
    H = 8
    print(min_eating_speed(piles, H))  
